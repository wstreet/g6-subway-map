{
  "extends": ["tslint:recommended", "tslint-react"],
  "rules": {
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-empty-interface": false,
    "curly": true,
    "no-var-requires": false,
    "member-ordering": false,
    "array-type": false,
    "no-string-literal": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "node_modules/**/*.tsx",
    ],
    "include": [
      "tslint.json"
    ]
  }
}
